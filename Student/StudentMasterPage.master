                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                    </li>

                </ul>
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <asp:LinkButton runat="server" ID="btnlogout" class="nav-link" OnClick="btnlogout_Click">Logout&nbsp;<i class="fas fa-sign-out-alt"></i></asp:LinkButton>
                    </li>
                </ul>
            </nav>
            <!-- /.navbar -->
            <!-- Main Sidebar Container -->
            <aside class="main-sidebar sidebar-dark-primary elevation-4">
                <!-- Brand Logo -->
               

                <div class="brand-link">                    
                    <div class="row">
                        <div class="col-3">
                            <img src="../design/img/c2.png" class="brand-image img-circle elevation-3">
                        </div>
                        <div class="col-9 brand-text">
                            <asp:Label runat="server" ID="lblloginname"></asp:Label><br />
                            <asp:Label runat="server" ID="lbldepartment"></asp:Label>
                        </div>
                        
                    </div>
                </div>

                <!-- Sidebar -->
                <div class="sidebar">


