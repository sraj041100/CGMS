                            <li class="nav-item">
                                <a href="StudentDashboard.aspx" class="nav-link">
                                    <p>
                                        Dashboard
                                    </p>
                                </a>

                            </li>

                            <li class="nav-item">
                                <a href="GrievanceRegistration.aspx" class="nav-link">
                                    <p>
                                        Register Grievance
                                    </p>
                                </a>

                            </li>

                        </ul>
                    </nav>
                    <!-- /.sidebar-menu -->
                </div>
                <!-- /.sidebar -->
